{
  "homepage": "https://marcuzgabriel.github.io/react-native-reanimated-skeleton/",
  "name": "@rnrs/monorepo",
  "author": "Marcuz Gabriel Larsen <marcuzgabriel@gmail.com>",
  "version": "1.6.0",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/app",
    "packages/web",
    "packages/npm"
  ],
  "scripts": {
    "build:npm": "yarn workspace react-native-reanimated-skeleton build",
    "clean-node-modules": "rm -rf node_modules && rm -rf packages/*/node_modules && yarn cache clean",
    "qa": "yarn format; yarn lint; yarn tsc",
    "format": "prettier --write --list-different '**/*.{json,ts,tsx}' --loglevel warn",
    "lint": "eslint . --fix --ext .ts,.tsx --no-cache",
    "lint:fix": "yarn lint --fix",
    "start": "yarn workspace @rnrs/app start",
    "storybook": "yarn workspace @rnrs/web storybook",
    "build-storybook": "yarn workspace @rnrs/web build-storybook",
    "type:check": "yarn tsc --noEmit",
    "type:generate:tsc": "yarn tsc",
    "type:generate:ios": "yarn type:generate:clean && yarn type:generate:create && yarn type:generate:tsc && yarn type:generate:delete:ios && yarn type:generate:copy:ios",
    "type:generate:clean": "cross-env-shell rimraf ./packages/npm/lib",
    "type:generate:create": "cross-env-shell mkdir ./packages/npm/lib",
    "type:generate:delete:ios": "rm ./packages/npm/lib/ReactNativeReanimatedSkeleton.js",
    "type:generate:copy:ios": "rsync -auv ./packages/npm/src/ReactNativeReanimatedSkeleton.ts ./packages/npm/lib/index.js"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.1",
    "@eslint/js": "^9.12.0",
    "@react-native/eslint-config": "^0.72.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^7.1.0",
    "@semantic-release/git": "^10.0.1",
    "@tsconfig/react-native": "^3.0.2",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.4",
    "@types/react": "^18.2.21",
    "@types/react-test-renderer": "^18.0.1",
    "@typescript-eslint/eslint-plugin": "^8.9.0",
    "@typescript-eslint/parser": "^8.9.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-eslint-plugin": "^6.2.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^7.1.0",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-native": "^4.1.0",
    "madge": "^8.0.0",
    "prettier": "^3.0.3",
    "react-native-builder-bob": "^0.23.2",
    "semantic-release": "21.1.1",
    "semantic-release-cli": "^5.4.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "5.2.2",
    "typescript-eslint": "^8.9.0"
  },
  "engines": {
    "node": ">=16"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
